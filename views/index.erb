<h1>Please insert text to encrypt </h1>



<form method="post" action="/">
  <div class="form-group">
    <label for="text">Text</label>
    <input id="text" name="text" class="form-control" placeholder="Text" />
  </div>
  <div class="form-group">
    <label for="Number">Number</label>
    <input id="Number" name="number" class="form-control" placeholder="Number" />
  </div>

  <button type="submit" class="btn btn-primary">Encrypt</button>
</form>
